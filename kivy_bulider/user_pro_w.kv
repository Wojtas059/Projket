<UserProWidget>
    name: 'userprowidget'
    canvas.before: 
        Color:
            rgba: (240/255,240/255,240/255,1)
        Rectangle:
            pos: self.pos
            size: self.size

    FloatLayout:
        size: root.width, root.height
        spacing: 20
        padding: 100
        Log:
            text: "Wróć"
            on_press: 
                root.manager.current = 'homewidget'
        

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        
        spacing: 20
        padding: 100
        

        Button:
            text: "Użytkownicy"
            #on_press: root.manager.current = 'singinwidget'

        Button:
            text: "Rozpocznij tryb Manual"

        Button:
            text: "Pomoc"
            on_press: root.manager.current = 'helpwidget'



<Button>
    size_hint: (0.5, 0.5)
    width: 200
    height: 50
    pos_hint: {'center_x': 0.5}

<Log@Button>
    size_hint: (0.1, 0.1)
    width: 100
    height: 25
    pos_hint: {'x': 0, 'top': 1}