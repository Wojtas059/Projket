<AChooseLotsMusclesWidget>
    name: 'a_chooselotsmuscleswidget'
    _mouscular_1:_mouscular_1
    _mouscular_2:_mouscular_2
    canvas.before: 
        Color:
            rgba: (240/255,240/255,240/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
    

    FloatLayout:
        size: root.width, root.height
        spacing: 20
        padding: 100

        Button:
            size_hint: (0.1, 0.1)
            pos_hint: {'x': 0, 'top': 1}
            text: "Wróć"
            on_press: 
                root.manager.transition.direction = 'right'
                root.manager.current = root.parent.last_screen()

        

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        
        spacing: 20
        padding: 250
        
        Spinner:
            size_hint: (0.5, 0.1)
            width: 200
            height: 50
            pos_hint: {'center_x': 0.5}

            id: many_
            text: 'Wybierz ilość badanych mięśni'
            values: '1', '2'
            on_text: root.choose_clicked( many_.text)




        Spinner:
            size_hint: (0.5, 0.1)
            width: 200
            height: 50
            pos_hint: {'center_x': 0.5}
            disabled: True
            id: _mouscular_1
            text: 'Wybierz partii mięśni'
            values: ''
            on_text: #root.choose_clicked(spinner_port.text)


        Spinner:
            size_hint: (0.5, 0.1)
            width: 200
            height: 50
            pos_hint: {'center_x': 0.5}
            disabled: True
            id: _mouscular_2
            text: 'Wybierz partii mięśni'
            values: ''
            on_text: #root.choose_clicked(spinner_port.text)

        
        UButton:
            text: "Dalej"
            
            on_press:
                if(root.on_click(many_.text)):\
                root.parent.add_screen('a_chooselotsmuscleswidget');\
                root.manager.current = 'a_managmentsensorsidget';\
                root.manager.transition.direction = 'left'

